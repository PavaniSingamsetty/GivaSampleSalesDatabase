CREATE TABLE Sales_Data (
    order_id INT PRIMARY KEY,
    skus_bought VARCHAR(255),
    price DECIMAL(10, 2),
    discount DECIMAL(5, 2),
    order_date DATE,
    customer_id INT,
    order_city VARCHAR(100),
    channel_type ENUM('online', 'offline')
);

